{
  "name": "discord-bot-fparma",
  "version": "2.0.0",
  "description": "FPARMA discord bot",
  "scripts": {
    "test": "yarn check:types && jasmine-ts \"./test/**/*.spec.ts\"",
    "test-watch": "chokidar -p './test/**/*' -c 'npm run test'",
    "start": "node -r ts-node/register -r dotenv/config index.ts",
    "setup-db": "docker run -d -p 27017-27019:27017-27019 --name mongodb mongo:3.6.12",
    "check:types": "tsc --noEmit"
  },
  "author": "Cuel",
  "license": "ISC",
  "dependencies": {
    "@types/gamedig": "^2.0.1",
    "discord.js": "12.4.1",
    "dotenv": "^4.0.0",
    "gamedig": "^2.0.25",
    "mongodb": "^2.2.36",
    "needle": "^2.5.2",
    "rimraf": "^2.6.1",
    "sanitize-filename": "^1.6.1",
    "ssh2-sftp-client": "^9.0.2",
    "ts-node": "^3.3.0",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.54",
    "@types/mongodb": "^2.2.11",
    "@types/needle": "^1.4.0",
    "@types/node": "^18.0.6",
    "@types/proxyquire": "^1.3.28",
    "@types/rimraf": "^2.0.2",
    "@types/sanitize-filename": "^1.1.28",
    "@types/ssh2-sftp-client": "^7.1.0",
    "chokidar-cli": "^2.1.0",
    "jasmine": "^2.8.0",
    "jasmine-ts": "^0.2.1",
    "proxyquire": "^1.8.0"
  }
}
